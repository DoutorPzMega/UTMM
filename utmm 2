local CraftSection = Tab:CreateSection("Craft")

local cbotoes = {}

for _, obj in ipairs(workspace:GetDescendants()) do
	if obj:IsA("BasePart") then
		local nome = obj.Name

		if nome:lower():find("craft") and nome ~= "Minecraft Stone Bricks" then
			table.insert(cbotoes, {
				Nome = nome,
				Part = obj
			})
		end
	end
end

table.sort(cbotoes, function(a, b)
	return a.Nome:lower() < b.Nome:lower()
end)

for _, info in ipairs(cbotoes) do
	Tab:CreateButton({
		Name = info.Nome,
		Callback = function()
			teleportTo(info.Part)
		end,
	})
end

local BattlesSection = Tab:CreateSection("Battles")

for _, obj in ipairs(workspace:GetDescendants()) do
	if obj:IsA("BasePart") then
		local scriptInside = obj:FindFirstChild("Script")

		if scriptInside and scriptInside:IsA("Script") then
			local battleValue = scriptInside:FindFirstChild("Battle") or scriptInside.Parent:FindFirstChild("Battle")

			if battleValue and battleValue:IsA("ObjectValue") and battleValue.Value then
				local buttonName
				local battleNameValue = battleValue.Value:FindFirstChild("BattleName")

				if battleNameValue and battleNameValue:IsA("StringValue") then
					buttonName = battleNameValue.Value
				else
					buttonName = battleValue.Value.Name
				end

				table.insert(var.battles, {
					Nome = buttonName,
					Part = obj
				})
			end
		end
	end
end

table.sort(var.battles, function(a, b)
	return a.Nome:lower() < b.Nome:lower()
end)

for _, boss in ipairs(var.battles) do
	Tab:CreateButton({
		Name = boss.Nome,
		Callback = function()
			teleportTo(boss.Part)
		end,
	})
end

local MiscTab = Window:CreateTab("Misc", 99586768718601)

local KeyPiecesF = Workspace.KeyPieces

local name = "Get 'Unlocked Badge' (1 click only)"

local GetUnlockedBadgePiece = MiscTab:CreateButton({
   Name = name,
   Callback = function()
		if name == "Get 'Unlocked Badge' (1 time only)" then
		 for i, v in KeyPiecesF:GetChildren() do
        	 if v and v:IsA("BasePart") and v:FindFirstChild("ClickDetector") then
                local clickpart = v.ClickDetector
                fireclickdetector(clickpart)
                print("a")
            end
        end
        task.wait()
        local click2 = workspace.KeyCraft.ClickDetector
        fireclickdetector(click2)
        print("Done")

		char.PrimaryPart.CFrame = CFrame.new(1295.26868, 828.862061, -6631.72168)

		name = "Done."
		end
   end,
})

